{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style" : "off",
        "no-unused-vars" : "off",
        "no-empty" : "off",
        "no-trailing-spaces" : "off",
        "no-irregular-whitespace" : "off",
        "consistent-return" : "off"
    }
}
